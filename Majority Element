  //Majority Element
                   int n= 5;
                   int arr[] = {3,1,3,3,2};
                   int temp=0;
                   for(int i=0;i<arr.length-1;i++){
                       for(int j=i+1;j<arr.length;j++){
                           if(arr[i]==arr[j]){
                               temp=arr[i];
                           }
                       }
                   }
                   System.out.print(temp);
