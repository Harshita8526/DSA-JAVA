//find max sub array sum

        int arr[]={1,2,3,-2,5};
        int arr[]={-2,1,-3,4,-1,2,1,-5,4};
        int cur_sum=0;
        int max_sum=0;

        for(int i=0;i<arr.length;i++){
          cur_sum=cur_sum+arr[i];
            if(cur_sum>0) {
                max_sum = cur_sum;
            }
            if(cur_sum<0){
                cur_sum=0;
           }

        }
